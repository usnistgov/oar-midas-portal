{
    "name": "midas-portal",
    "workspaces": [
        "lib",
        "midas-portal"
    ],
    "scripts": {
        "build-lib": "npm run build oarng --prefix ./lib",
        "build-all": "npm run build oarng --prefix ./lib && npm run build midas-portal --prefix ./midas-portal",
        "build": "npm run build midas-portal --prefix ./midas-portal",
        "start": "npm run start midas-portal --prefix ./midas-portal",
        "test": "jest --config ./midas-portal/jest.config.js --silent",
        "test:watch": "jest --watch",
        "test:file": "jest --testPathPattern"
    },
    "dependencies": {
        "@fortawesome/angular-fontawesome": "^0.10.2",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@ionic/angular": "^6.3.8",
        "ngx-csv": "^0.3.2",
        "pdfmake": "^0.2.10",
        "primefaces": "^12.0.0",
        "primeng": "^16.4.3"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/pdfmake": "^0.2.9",
        "jest": "^29.7.0",
        "jest-preset-angular": "^13.1.6",
        "ts-jest": "^29.2.5"
    }
}
